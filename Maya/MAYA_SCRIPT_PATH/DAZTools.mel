    if ($folder==""){
	    if (`filetest -d "C:/Daz/Applications/Data/DAZ 3D/My DAZ 3D Library/People/Genesis 8 Male/Animation/"`) $folder = "C:/Daz/Applications/Data/DAZ 3D/My DAZ 3D Library/People/Genesis 8 Male/Animation/";
