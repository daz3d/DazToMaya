	switch ($color)
	{
		case "red":
			$value = 13;
		break;
		
		case "blue": 
			$value = 6;
		break;

		case "yellow":
			$value = 17;
		break;

		case "darkRed":
			$value = 4;
		break;

		case "darkBlue":
			$value = 5;
		break; 
		
		case "babyBlue":
			$value = 18;
		break; 
		
		case "darkPurple":
			$value = 8;
		break; 
		
		case "darkYellow":
			$value = 25;
		break; 
			
		case "darkGreen":
			$value = 23;
		break; 
			
		case "brown":
			$value = 10;
		break; 
			
		case "lightBrown":
			$value = 21;
		break; 
			
		case "lightPink":
			$value = 20;
		break; 
			
		case "darkPink":
			$value = 31;
		break; 
			
		default:
			$value = 2;
		break; 
		
